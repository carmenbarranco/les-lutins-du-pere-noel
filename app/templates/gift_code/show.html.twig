{% block title %}GiftCode{% endblock %}

{% block body %}
    <h1>GiftCode</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gift_code.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ gift_code.code }}</td>
            </tr>
