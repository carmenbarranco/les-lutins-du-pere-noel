            <tr>
                <th>UpdatedAt</th>
                <td>{{ gift_code.updatedAt ? gift_code.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('gift_code_index') }}">back to list</a>

    <a href="{{ path('gift_code_edit', {'id': gift_code.id}) }}">edit</a>

    {{ include('gift_code/_delete_form.html.twig') }}
